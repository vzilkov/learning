{
    "configurations": [

        {
            "name": "Launch boost example http parser",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/boost_example.out",
            "args": [
                "www.example.com",
                "80",
                "/",
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
    ],
    "version": "2.0.0"
}